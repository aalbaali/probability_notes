	date = {2001-12},
	note = {{ISSN}: 1063-6919},
	keywords = {automatic image stitching, Brightness, Displays, dynamic scene, exposure artifacts, exposure corrections, ghosting elimination, Head, high-quality software, image mosaics, Image motion analysis, image segmentation, Layout, Optical computing, Optical filters, panoramic photography, Photography, Pixel, spatially continuous exposure adjustment, spline, splines (mathematics)},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\albaa\\Zotero\\storage\\DK6PDQBS\\991005.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\albaa\\Zotero\\storage\\DUAZA9DP\\Uyttendaele et al. - 2001 - Eliminating ghosting and exposure artifacts in ima.pdf:application/pdf}
