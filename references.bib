@thesis{albaaliParallelFeedforwardControl,
  title = {Parallel {{Feedforward Control Using Linear Matrix Inequalities}}},
  author = {Al Baali, Amro},
  abstract = {In this thesis, output modification is used to modify the output of an asymptotically stable, nonminimum phase (NMP), linear time-invariant (LTI) plant to be minimum phase using parallel feedforward control and for the output to be as similar as possible to the NMP plant output. The feedforward controller is constructed by first constructing an invertible feedback controller that asymptotically stabilizes the NMP plant, and then setting the feedforward controller to be the inverse of the feedback controller. In the first part of this thesis, pole-placement and nonlinear optimization are used to construct an H2-optimal feedback controller that shapes the poles of the closed loop of the NMP plant for the given poles and for the given feedback controller order and relative degree. In the second part of this thesis, a linear matrix-inequality (LMI) formulation is presented to construct a strictly positive real (SPR), invertible, dynamic feedback controller that renders the closed loop of the NMP plant asymptotically stable using the Lyapunov inequality and the Kalman-Yakubovich-Popov (KYP) Lemma. A similar LMI formulation is then presented to construct an extended strictly positive real (ESPR) dynamic feedback controller that renders the closed loop descriptor system of the NMP plant admissible using the Generalized Lyapunov Equation and the KYP Lemma for descriptor systems.},
  langid = {english},
  file = {/home/aa/Documents/T7/Zotero/storage/C9YINFZP/Baali - Parallel Feedforward Control Using Linear Matrix I.pdf}
}

