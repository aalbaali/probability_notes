@online{shiROBINGraphTheoreticApproach2021,
  title = {{{ROBIN}}: A {{Graph-Theoretic Approach}} to {{Reject Outliers}} in {{Robust Estimation}} Using {{Invariants}}},
  shorttitle = {{{ROBIN}}},
  author = {Shi, Jingnan and Yang, Heng and Carlone, Luca},
