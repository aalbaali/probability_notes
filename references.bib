	keywords = {{SLAM}, clique tree, graphical models, {iSAM}, nonlinear optimization, probabilistic inference, smoothing and mapping, sparse linear algebra},
	file = {Springer Full Text PDF:C\:\\Users\\albaa\\Zotero\\storage\\2ZGPQXQG\\Kaess et al. - 2011 - The Bayes Tree An Algorithmic Foundation for Prob.pdf:application/pdf}
