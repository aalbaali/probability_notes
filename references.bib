	eprinttype = {arxiv},
	eprint = {1812.01892},
	keywords = {Mathematics - Numerical Analysis},
	file = {arXiv Fulltext PDF:C\:\\Users\\albaa\\Zotero\\storage\\XZN92DHY\\Rackauckas et al. - 2018 - A Comparison of Automatic Differentiation and Cont.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\albaa\\Zotero\\storage\\U49DEQ3W\\1812.html:text/html}
