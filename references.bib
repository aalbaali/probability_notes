@inproceedings{ilaFastCovarianceRecovery2015,
  title = {Fast Covariance Recovery in Incremental Nonlinear Least Square Solvers},
  booktitle = {2015 {{IEEE International Conference}} on {{Robotics}} and {{Automation}} ({{ICRA}})},
  author = {Ila, Viorela and Polok, Lukas and Solony, Marek and Smrz, Pavel and Zemcik, Pavel},
