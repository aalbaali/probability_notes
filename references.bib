	keywords = {Computer Science - Robotics, Mathematics - Optimization and Control},
	file = {arXiv Fulltext PDF:C\:\\Users\\albaa\\Zotero\\storage\\YMN8TS2A\\Tian et al. - 2020 - Distributed Certifiably Correct Pose-Graph Optimiz.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\albaa\\Zotero\\storage\\QAN6J7JS\\1911.html:text/html}
}

